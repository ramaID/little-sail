# -- Aliases ------------------------------------------
alias a="php artisan"
alias art="php artisan"
alias migrate="php artisan migrate"
alias seed="php artisan db:seed"
alias fresh="php artisan migrate:fresh"
alias psalm="vendor/bin/psalm --no-file-cache --threads=1 --no-diff --show-info=true --find-unused-psalm-suppress --long-progress"
alias c="composer"
alias test="php artisan test"
alias routes="php artisan route:list --columns=Name,Method,URI,Middleware"
